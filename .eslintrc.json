{
    "env": {
    "es2021": true,
        "node": true
},
    "plugins": ["@stylistic"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended","plugin:@stylistic/recommended-extends"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    "ecmaVersion": "latest",
        "sourceType": "module"
},
    "rules": {
        "@stylistic/indent": ["error", 4]
}
}
